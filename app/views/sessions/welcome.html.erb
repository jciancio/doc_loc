    <!-- About Section -->
    <section class="success" id="about">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2>About</h2>
                    <hr class="star-light">
                </div>
            </div>
            <div class="row">
                <div class="col-lg-4 col-lg-offset-2">
                    <p>Doctor finder is a free to use online platform that allows users to search through a database of over a millions doctors and view their practice location on a map in real time</p>
                </div>
                <div class="col-lg-4">
                    <p>We are also creating a community for individuals to post about their experiences and discover new information through new relevant content</p>
                </div>
                <div class="col-lg-8 col-lg-offset-2 text-center">
                    <%= link_to 'Get Started', new_user_path, class: "btn btn-lg btn-outline" %>
                </div>
            </div>
        </div>
    </section>

    
