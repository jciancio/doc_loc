<div class="container col-md-4 text-center">
  <%= form_tag sessions_path, id: "signup" do %>
  <div class="header">
    <h3>Welcome Back</h3>
    <p>Please Sign In Below</p>
  </div>
  <div class="sep"></div>
  <div class="inputs">
    <%= email_field_tag :email, nil ,placeholder:'Email', autofocus: true %>
    <%= password_field_tag :password, nil ,placeholder:'Password' %>
    <%= submit_tag 'Sign in', class: "login" %>
  </div>
  <% end %>
</div>

